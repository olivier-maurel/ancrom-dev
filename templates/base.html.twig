						<div class="card-head d-flex justify-content-between position-relative">
							<div id="win_app_head" class="col-12">

							</div>
							<div class="position-absolute top-0 end-0 d-flex justify-content-end">
								<span id="win_app_close" class="btn btn-sm btn-os text-white-soft text-code fw-bold">
									<i class="fa fa-times text-white-soft opacity-75 p-0" style="font-size: 1.2rem !important;"></i>
								</span>
							</div>
						</div>
						<div id="win_app_body" class="card-body overflow-hidden h-100 p-0 m-5">
							
						</div>
					</div>
