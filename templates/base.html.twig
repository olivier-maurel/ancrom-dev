    <body id="kt_body" class="toolbar-enabled overflow-hidden bg-body" data-kt-sticky-header="on" data-kt-header="on" data-kt-scrolltop="on" data-float-page="off" style="background-image: url({{ asset('img/bg/bg-'~ ((setting.background is defined) ? setting.background : '8') ~'.jpg') }});">        
        <div id="container" class="d-flex flex-column flex-root">
			<!--begin::Page-->
			<div class="page d-flex flex-column flex-column-fluid">
				
				<!--begin::Wrapper-->
				<div class="wrapper d-flex flex-column flex-row-fluid container-xxl h-100 align-items-center justify-content-end " id="kt_wrapper">
					<!--begin::Main-->
					<div class="topbar py-2 d-flex align-items-stretch flex-shrink-0" id="kt_topbar" style="z-index: 999;">
						<!--begin::Activities-->
						<div class="d-flex align-items-center mx-4 mx-lg-5">
							<!--begin::Drawer toggle-->
							<a class="btn btn-icon btn-custom w-lg-50px h-lg-50px w-50px h-50px w-sm-90px h-sm-90px flex-column" data-page="profile" data-pos="left" id="kt_activities_toggle">
								<!--begin::Svg Icon | path: icons/duotune/general/gen032.svg-->
								<span class="svg-icon svg-icon-1 d-flex align-items-center flex-column">
									<i class="fa fa-user"></i>
								</span>
								<span class="focus">●</span>
								<!--end::Svg Icon-->
							</a>
							<!--end::Drawer toggle-->
						</div>
						<!--end::Activities-->
						<!--begin::Activities-->
						<div class="d-flex align-items-center  mx-4 mx-lg-5">
							<!--begin::Drawer toggle-->
							<a class="btn btn-icon btn-custom w-lg-50px h-lg-50px w-50px h-50px w-sm-90px h-sm-90px" data-page="project" data-pos="bottom" id="kt_activities_toggle">
								<!--begin::Svg Icon | path: icons/duotune/general/gen032.svg-->
								<span class="svg-icon svg-icon-1 d-flex align-items-center flex-column">
									<i class="fa fa-flask"></i>
								</span>
								<span class="focus">●</span>
								<!--end::Svg Icon-->
							</a>
							<!--end::Drawer toggle-->
						</div>
						<!--end::Activities-->
						<!--begin::Activities-->
						<div class="d-flex align-items-center  mx-4 mx-lg-5">
							<!--begin::Drawer toggle-->
							<a class="btn btn-icon btn-custom w-lg-50px h-lg-50px w-50px h-50px w-sm-90px h-sm-90px" data-page="contact" data-pos="right" id="kt_activities_toggle">
								<!--begin::Svg Icon | path: icons/duotune/general/gen032.svg-->
								<span class="svg-icon svg-icon-1 d-flex align-items-center flex-column">
									<i class="fa fa-envelope"></i>
								</span>
								<span class="focus">●</span>
								<!--end::Svg Icon-->
							</a>
							<!--end::Drawer toggle-->
						</div>
						<!--end::Activities-->
					</div>
					<!--end::Main-->
					<div id="welcome" class="float-page init-page" style="margin-top: 30px;">
