							<div class="alert bg-light position-relative">
								<div class="position-absolute top-0 end-0 d-flex justify-content-end">
									<span id="welcome_close" class="btn btn-sm btn-os text-white-soft text-code fw-bold">
										<i class="fa fa-times text-white-soft opacity-75 p-0" style="font-size: 1.2rem !important;"></i>
									</span>
								</div>
								<h2 class="m-0 p-0">Welcome to ancrom.com</h2>
								<small>the website developped by Olivier Maurel</small>
								<p>Bienvenue sur mon site portfolio</p>
							</div>
						</div>
					</div>
				</div>
				<!--end::Wrapper-->
			</div>
			<!--end::Page-->
			<div id="profile" data-load="false" class="page d-flex flex-column flex-column-fluid float-page float-page-left col-lg-8 m-auto" style="z-index: 10;">
				<div class="col-lg-12 m-auto h-100 p-9">

				</div>
			</div>
			<div id="contact" data-load="false" class="page d-flex flex-column flex-column-fluid float-page float-page-right col-lg-8 m-auto" style="z-index: 10;">
